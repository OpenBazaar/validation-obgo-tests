{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "queryCount",
    "sales"
  ],
  "properties": {
    "queryCount": {
      "$id": "#/properties/queryCount",
      "type": "integer",
      "title": "The Querycount Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "sales": {
      "$id": "#/properties/sales",
      "type": "array",
      "title": "The Sales Schema",
      "items": {
        "$id": "#/properties/sales/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "buyerHandle",
          "buyerId",
          "coinType",
          "moderated",
          "orderId",
          "paymentCoin",
          "read",
          "shippingAddress",
          "shippingName",
          "slug",
          "state",
          "thumbnail",
          "timestamp",
          "title",
          "total",
          "unreadChatMessages"
        ],
        "properties": {
          "buyerHandle": {
            "$id": "#/properties/sales/items/properties/buyerHandle",
            "type": "string",
            "title": "The Buyerhandle Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "buyerId": {
            "$id": "#/properties/sales/items/properties/buyerId",
            "type": "string",
            "title": "The Buyerid Schema",
            "default": "",
            "examples": [
              "QmXMT7kT4FD2s3u8z34ETMzJZSfy9nBn3q3E4VVg26UNPy"
            ],
            "pattern": "^(.*)$"
          },
          "coinType": {
            "$id": "#/properties/sales/items/properties/coinType",
            "type": "string",
            "title": "The Cointype Schema",
            "default": "",
            "examples": [
              "USD"
            ],
            "pattern": "^(.*)$"
          },
          "moderated": {
            "$id": "#/properties/sales/items/properties/moderated",
            "type": "boolean",
            "title": "The Moderated Schema",
            "default": false,
            "examples": [
              false
            ]
          },
          "orderId": {
            "$id": "#/properties/sales/items/properties/orderId",
            "type": "string",
            "title": "The Orderid Schema",
            "default": "",
            "examples": [
              "QmbNHboEnuEEJmYWEpEuytVbrb9puhPmUJXJha7BVDPLt2"
            ],
            "pattern": "^(.*)$"
          },
          "paymentCoin": {
            "$id": "#/properties/sales/items/properties/paymentCoin",
            "type": "string",
            "title": "The Paymentcoin Schema",
            "default": "",
            "examples": [
              "ETH"
            ],
            "pattern": "^(.*)$"
          },
          "read": {
            "$id": "#/properties/sales/items/properties/read",
            "type": "boolean",
            "title": "The Read Schema",
            "default": false,
            "examples": [
              true
            ]
          },
          "shippingAddress": {
            "$id": "#/properties/sales/items/properties/shippingAddress",
            "type": "string",
            "title": "The Shippingaddress Schema",
            "default": "",
            "examples": [
              "A "
            ],
            "pattern": "^(.*)$"
          },
          "shippingName": {
            "$id": "#/properties/sales/items/properties/shippingName",
            "type": "string",
            "title": "The Shippingname Schema",
            "default": "",
            "examples": [
              "N"
            ],
            "pattern": "^(.*)$"
          },
          "slug": {
            "$id": "#/properties/sales/items/properties/slug",
            "type": "string",
            "title": "The Slug Schema",
            "default": "",
            "examples": [
              "t1"
            ],
            "pattern": "^(.*)$"
          },
          "state": {
            "$id": "#/properties/sales/items/properties/state",
            "type": "string",
            "title": "The State Schema",
            "default": "",
            "examples": [
              "COMPLETED"
            ],
            "pattern": "^(.*)$"
          },
          "thumbnail": {
            "$id": "#/properties/sales/items/properties/thumbnail",
            "type": "string",
            "title": "The Thumbnail Schema",
            "default": "",
            "examples": [
              "QmVh3jegqw9V4DyYNiXMp77SchVZTACGZDe8PW1oQak9vY"
            ],
            "pattern": "^(.*)$"
          },
          "timestamp": {
            "$id": "#/properties/sales/items/properties/timestamp",
            "type": "string",
            "title": "The Timestamp Schema",
            "default": "",
            "examples": [
              "2019-10-23T19:34:43Z"
            ],
            "pattern": "^(.*)$"
          },
          "title": {
            "$id": "#/properties/sales/items/properties/title",
            "type": "string",
            "title": "The Title Schema",
            "default": "",
            "examples": [
              "T1"
            ],
            "pattern": "^(.*)$"
          },
          "total": {
            "$id": "#/properties/sales/items/properties/total",
            "type": "string",
            "title": "The Total Schema",
            "default": "",
            "examples": [
              "3170898315200000"
            ],
            "pattern": "^(.*)$"
          },
          "unreadChatMessages": {
            "$id": "#/properties/sales/items/properties/unreadChatMessages",
            "type": "integer",
            "title": "The Unreadchatmessages Schema",
            "default": 0,
            "examples": [
              0
            ]
          }
        }
      }
    }
  }
}
